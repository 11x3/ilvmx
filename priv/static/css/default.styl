@import "bootflat.css"
@import "jeet"

html
  font sans
  color #fff
  font-size 150%
  background-color #212121

body
  margin 0
  padding 0
  background-color #080808

header
  padding 20px

#page
  background-color #080808


#nubspace
  color #6010a3
  background-color #000
  font-size 28px
  border-radius 3px
  border #212188 1px solid
  padding 10px

.row
  span(1/5)
  min-height 400px
  max-height 400px
  overflow hidden
  margin-left -1px
  margin-right 0
  padding 20px
  border #131313 1px solid
  background-color #020808
  
img
  span(1/2)
  
footer
  cf()