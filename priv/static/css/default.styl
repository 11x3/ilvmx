@import "jeet"
@import "bootflat.css"

html
  font sans
  color #fff
  font-size 150%
  background-color #212121

body
  margin 0
  padding 0
  background-color #080808

#nubspace
  color #423321
  background-color #000
  font-size 42px
  border-radius 3px
  border #212188 1px solid
  padding 20px

.row
  col(1/5)
  margin-left -1px
  margin-right 0
  padding 20px
  border #131313 1px solid
  background-color #080808
  
img
  span(1/2)